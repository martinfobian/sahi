function googleMaps(j,a,s,p,l,h){var c=s;var q=p;var f=true;if(((!s)&&(!p))&&((l)&&(h)&&(l!="")&&(h!=""))){c=l;q=h;f=false}if((c)&&(q)&&(c!="")&&(q!="")){geocoder=new google.maps.Geocoder();var i=new google.maps.LatLng(c,q);var n={zoom:a,disableDefaultUI:true,zoomControl:true,mapTypeId:google.maps.MapTypeId.ROADMAP};if((!l)||(!h)||(!s)||(!p)){n.center=i}var r=new google.maps.Map(document.getElementById("map_canvas"),n);if(f){var m=new google.maps.MarkerImage(j+"elements/map-marker.png",new google.maps.Size(29,42),new google.maps.Point(0,0),new google.maps.Point(16,42));var e=new google.maps.MarkerImage(j+"elements/map-marker-shadow.png",new google.maps.Size(53,42),new google.maps.Point(0,0),new google.maps.Point(17,42));var o=new google.maps.Marker({position:i,icon:m,shadow:e,map:r})}else{var b=new google.maps.Marker({position:i,map:r})}if((s)&&(p)&&(l)&&(h)&&(l!="")&&(h!="")){var d=new google.maps.LatLng(l,h);var k=new google.maps.DirectionsService();var g={origin:d,destination:i,travelMode:google.maps.TravelMode.DRIVING};k.route(g,function(t,u){if(u==google.maps.DirectionsStatus.OK){var v=new google.maps.Marker({position:d,map:r});var x={map:r,suppressMarkers:true};var w=new google.maps.DirectionsRenderer(x);w.setMap(r);w.setDirections(t)}})}}};
